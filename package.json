    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "express-flash-messages": "^0.1.1",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "if-env": "^1.0.4",
    "mongod": "^2.0.0",
    "mongoose": "^5.0.17",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
